<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@xml/login"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteX="-3dp"
    tools:layout_editor_absoluteY="-38dp">

    <ImageView
        android:id="@+id/imgShopping"
        android:layout_width="237dp"
        android:layout_height="266dp"
        android:src="@xml/circle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.27"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.247"
        tools:ignore="ImageContrastCheck"
        tools:srcCompat="@xml/circle" />

    <TextView
        android:id="@+id/txtShopping"
        android:layout_width="119dp"
        android:layout_height="54dp"
        android:text="@string/shopping"
        android:textColor="#fff"
        app:layout_constraintBottom_toBottomOf="@+id/imgShopping"
        app:layout_constraintEnd_toEndOf="@+id/imgShopping"
        app:layout_constraintHorizontal_bias="0.347"
        app:layout_constraintStart_toStartOf="@+id/imgShopping"
        app:layout_constraintTop_toTopOf="@+id/imgShopping"
        app:layout_constraintVertical_bias="0.216" />

    <ImageView
        android:id="@+id/imgPanier"
        android:layout_width="194dp"
        android:layout_height="107dp"
        android:src="@drawable/panier"
        app:layout_constraintBottom_toBottomOf="@+id/imgShopping"
        app:layout_constraintEnd_toEndOf="@+id/imgShopping"
        app:layout_constraintHorizontal_bias="0.139"
        app:layout_constraintStart_toStartOf="@+id/imgShopping"
        app:layout_constraintTop_toTopOf="@+id/imgShopping"
        app:layout_constraintVertical_bias="0.496"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/imgLogin"
        android:layout_width="26dp"
        android:layout_height="50dp"
        android:src="@drawable/user"
        app:layout_constraintBottom_toTopOf="@+id/imgShopping"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.783"
        app:layout_constraintStart_toEndOf="@+id/imgShopping"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.039" />

    <ImageView
        android:id="@+id/imgProducts"
        android:layout_width="184dp"
        android:layout_height="169dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.784"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.523"
        app:srcCompat="@xml/secondcircle" />

    <ImageView
        android:id="@+id/imgMoney"
        android:layout_width="115dp"
        android:layout_height="120dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.741"
        app:srcCompat="@drawable/money" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="83dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgProducts"
        app:layout_constraintEnd_toEndOf="@+id/imgProducts"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imgProducts"
        app:layout_constraintTop_toTopOf="@+id/imgProducts"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/bottle" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="84dp"
        android:layout_height="76dp"
        android:layout_marginBottom="80dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgProducts"
        app:layout_constraintEnd_toEndOf="@+id/imgProducts"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:srcCompat="@drawable/orangejuice_106708" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="70dp"
        android:layout_height="99dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgProducts"
        app:layout_constraintEnd_toEndOf="@+id/imgProducts"
        app:layout_constraintHorizontal_bias="0.824"
        app:layout_constraintStart_toStartOf="@+id/imgProducts"
        app:layout_constraintTop_toTopOf="@+id/imgProducts"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/pills_pill_bottle_medicine_medical_healthcare_icon_141993" />

    <TextView
        android:id="@+id/txtProducts"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:layout_marginTop="100dp"
        android:text="@string/products"
        android:textColor="#fff"
        app:layout_constraintBottom_toBottomOf="@+id/imgProducts"
        app:layout_constraintEnd_toEndOf="@+id/imgProducts"
        app:layout_constraintHorizontal_bias="0.254"
        app:layout_constraintStart_toStartOf="@+id/imgProducts"
        app:layout_constraintTop_toTopOf="@+id/imgProducts"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imgGuideMe"
        android:layout_width="242dp"
        android:layout_height="227dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView10"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imgPanier"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.871"
        app:srcCompat="@xml/guidecircle" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="99dp"
        android:layout_height="95dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgGuideMe"
        app:layout_constraintEnd_toEndOf="@+id/imgGuideMe"
        app:layout_constraintHorizontal_bias="0.306"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgGuideMe"
        app:layout_constraintVertical_bias="0.553"
        app:srcCompat="@drawable/blindmans" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="96dp"
        android:layout_height="69dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgGuideMe"
        app:layout_constraintEnd_toEndOf="@+id/imgGuideMe"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgGuideMe"
        android:layout_marginLeft="10dp"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/guidedog_99772" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="119dp"
        android:layout_height="112dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgGuideMe"
        app:layout_constraintEnd_toEndOf="@+id/imgGuideMe"
        app:layout_constraintHorizontal_bias="0.943"
        app:layout_constraintStart_toStartOf="@+id/imgGuideMe"
        app:layout_constraintTop_toTopOf="@+id/imgGuideMe"
        app:layout_constraintVertical_bias="0.4"
        app:srcCompat="@drawable/direction" />

    <TextView
        android:id="@+id/txtGuideMe"
        android:layout_width="129dp"
        android:layout_height="35dp"
        android:text="@string/guide"
        android:textColor="#fff"
        app:layout_constraintBottom_toBottomOf="@+id/imgGuideMe"
        app:layout_constraintEnd_toEndOf="@+id/imgGuideMe"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="@+id/imgGuideMe"
        app:layout_constraintTop_toTopOf="@+id/imgGuideMe"
        app:layout_constraintVertical_bias="0.125" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="67dp"
        android:layout_height="65dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgMoney"
        app:layout_constraintEnd_toEndOf="@+id/imgMoney"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imgMoney"
        app:layout_constraintTop_toTopOf="@+id/imgMoney"
        app:layout_constraintVertical_bias="0.818"
        app:srcCompat="@drawable/dollar" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="67dp"
        android:layout_height="65dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgMoney"
        app:layout_constraintEnd_toEndOf="@+id/imgMoney"
        app:layout_constraintHorizontal_bias="0.083"
        app:layout_constraintStart_toStartOf="@+id/imgMoney"
        app:layout_constraintTop_toTopOf="@+id/imgMoney"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/moneybag" />

    <TextView
        android:id="@+id/txtMoney"
        android:layout_width="89dp"
        android:layout_height="60dp"
        android:text="@string/money"
        android:textColor="#fff"
        app:layout_constraintBottom_toBottomOf="@+id/imgMoney"
        app:layout_constraintEnd_toEndOf="@+id/imgMoney"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imgMoney"
        app:layout_constraintTop_toTopOf="@+id/imgMoney"
        app:layout_constraintVertical_bias="0.283" />

</androidx.constraintlayout.widget.ConstraintLayout>